{
  "extends": "@tsconfig/node20",
  "compilerOptions": {
    "isolatedModules": true,
    "declaration": false,
    "sourceMap": true,
    "noEmit": true,
    "noUncheckedIndexedAccess": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node"
  }
}
